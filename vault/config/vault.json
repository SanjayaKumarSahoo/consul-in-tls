{
  "backend": {
    "file": {
      "path": "/vault/file"
    }
  },
  "listener": {
    "tcp":{
      "address": "0.0.0.0:8200",
      "tls_disable": 0,
      "tls_cert_file": "/vault/certs/server.crt",
      "tls_key_file" : "/vault/certs/server.key",
      "tls_disable_client_certs": false
    }
  },
  "ui": true
}